{
  "name": "bot",
  "version": "2.26.2",
  "scripts": {
    "build": "babel src --out-dir dist --copy-files",
    "start": "NODE_ENV=production node dist/index.js",
    "start:dev": "node -r dotenv/config dist/index.js",
    "dev": "nodemon --exec babel-node -r dotenv/config src/index.js",
    "dev:debug": "nodemon --exec babel-node --inspect=0.0.0.0 -r dotenv/config src/index.js",
    "test": "jest",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand --watch",
    "deploy": "yarn build && docker build . -t appmanagm/agm-on-chat:bot && docker push appmanagm/agm-on-chat:bot",
    "lint": "eslint ./src"
  },
  "dependencies": {
    "@appman/mac_modules": "git+ssh://git@github.com:appman-agm/mac_modules.git#40dc694",
    "@sentry/node": "^6.11.0",
    "@sentry/tracing": "^6.11.0",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-http": "^1.5.16",
    "apollo-link-ws": "^1.0.19",
    "aws-appsync": "^3.0.2",
    "aws-appsync-auth-link": "^2.0.1",
    "aws-appsync-subscription-link": "^2.0.1",
    "aws-sdk": "^2.655.0",
    "axios": "^0.26.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.10.3",
    "i18next": "^19.3.3",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "node-cache": "^5.1.0",
    "node-redis": "https://github.com/NodeRedis/node_redis.git#v.2.8.0",
    "nohm": "^2.2.3",
    "query-string": "^7.0.1",
    "subscriptions-transport-ws": "^0.9.16",
    "winston": "^3.3.3",
    "winston-transport-sentry-node": "^1.0.2",
    "xstate": "^4.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "babel-eslint": "^10.0.2",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.19.1"
  }
}
